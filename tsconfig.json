{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "types": [
            "react",
            "react-dom"
        ],
        "importHelpers": true,
        "moduleResolution": "node",
        "declaration": false,
        "esModuleInterop": true,
        "noImplicitAny": false,
        "jsx": "react",
        "jsxFactory": "wp.element.createElement",
        "sourceMap": true,
        "noLib": false,
        "suppressImplicitAnyIndexErrors": true
    },
    "compileOnSave": false,
    "exclude": [
        "node_modules",
        "vendor"
    ]
}